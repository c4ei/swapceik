		"devdoc": {
			"methods": {
				"allowance(address,address)": {
					"details": "Get allowed amount for an account\r",
					"params": {
						"owner": "address The account owner\r",
						"spender": "address The account spender\r"
					}
				},
				"approve(address,uint256)": {
					"details": "Adds blacklisted check to approve\r",
					"return": "True if the operation was successful.\r"
				},
				"balanceOf(address)": {
					"details": "Get token balance of an account\r",
					"params": {
						"account": "address The account\r"
					}
				},
				"blacklist(address)": {
					"details": "Adds account to blacklist\r",
					"params": {
						"_account": "The address to blacklist\r"
					}
				},
				"burn(uint256)": {
					"details": "allows a minter to burn some of its own tokens\r Validates that caller is a minter and that sender is not blacklisted\r amount is less than or equal to the minter's account balance\r",
					"params": {
						"_amount": "uint256 the amount of tokens to be burned\r"
					}
				},
				"configureMinter(address,uint256)": {
					"details": "Function to add/update a new minter\r",
					"params": {
						"minter": "The address of the minter\r",
						"minterAllowedAmount": "The minting amount allowed for the minter\r"
					},
					"return": "True if the operation was successful.\r"
				},
				"isBlacklisted(address)": {
					"details": "Checks if account is blacklisted\r",
					"params": {
						"_account": "The address to check    \r"
					}
				},
				"isMinter(address)": {
					"details": "Checks if account is a minter\r",
					"params": {
						"account": "The address to check    \r"
					}
				},
				"mint(address,uint256)": {
					"details": "Function to mint tokens\r",
					"params": {
						"_amount": "The amount of tokens to mint. Must be less than or equal to the minterAllowance of the caller.\r",
						"_to": "The address that will receive the minted tokens.\r"
					},
					"return": "A boolean that indicates if the operation was successful.\r"
				},
				"minterAllowance(address)": {
					"details": "Get minter allowance for an account\r",
					"params": {
						"minter": "The address of the minter\r"
					}
				},
				"owner()": {
					"details": "Tells the address of the owner\r",
					"return": "the address of the owner\r"
				},
				"pause()": {
					"details": "called by the owner to pause, triggers stopped state\r"
				},
				"removeMinter(address)": {
					"details": "Function to remove a minter\r",
					"params": {
						"minter": "The address of the minter to remove\r"
					},
					"return": "True if the operation was successful.\r"
				},
				"totalSupply()": {
					"details": "Get totalSupply of token\r"
				},
				"transfer(address,uint256)": {
					"details": "transfer token for a specified address\r",
					"params": {
						"_to": "The address to transfer to.\r",
						"_value": "The amount to be transferred.\r"
					},
					"return": "bool success\r"
				},
				"transferFrom(address,address,uint256)": {
					"details": "Transfer tokens from one address to another.\r",
					"params": {
						"_from": "address The address which you want to send tokens from\r",
						"_to": "address The address which you want to transfer to\r",
						"_value": "uint256 the amount of tokens to be transferred\r"
					},
					"return": "bool success\r"
				},
				"transferOwnership(address)": {
					"details": "Allows the current owner to transfer control of the contract to a newOwner.\r",
					"params": {
						"newOwner": "The address to transfer ownership to.\r"
					}
				},
				"unBlacklist(address)": {
					"details": "Removes account from blacklist\r",
					"params": {
						"_account": "The address to remove from the blacklist\r"
					}
				},
				"unpause()": {
					"details": "called by the owner to unpause, returns to normal state\r"
				},
				"updatePauser(address)": {
					"details": "update the pauser role\r"
				}
